{
	"name": "stapostol/morphy-laravel",
	"description": "Analyze text with morphy on php.",
	"type": "library",
	"autoload": {
		"psr-4": {
			"LaravelMorphy\\": "./src"
		}
	},
	"require": {
		"php": ">=8.0",
		"illuminate/support": "^8.0",
		"resolventa/morphological-search": "^1.0"
	},
	"minimum-stability": "dev",
	"extra": {
		"laravel": {
			"providers": [
				"LaravelMorphy\\MorphServiceProvider"
			]
		}
	}
}
