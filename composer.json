{
	"name": "stapostol/morphy-laravel",
	"description": "Analyze text with morphy on php.",
	"type": "library",
	"autoload": {
		"psr-4": {
			"LaravelMorphy\\": "./src"
		}
	},
	"require": {
		"illuminate/support": "^6.0|^7.0|^8.0",
		"stapostol/morph-analyzer": "^0.0.1"
	},
	"minimum-stability": "dev",
	"extra": {
		"laravel": {
			"providers": [
				"LaravelMorphy\\MorphServiceProvider"
			]
		}
	},
	"repositories": [
		{
			"type": "git",
			"url": "git@github.com:StApostol/morph-analyzer.git"
		}
	]
}
